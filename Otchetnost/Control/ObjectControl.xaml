<UserControl x:Class="Otchetnost.ObjectControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Otchetnost"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel Tag="1" Orientation="Vertical" Height="35" MouseEnter="Object_MouseEnter" MouseLeave="Object_MouseLeave">
            <TextBlock x:Name="ObjectName"  FontSize="15" Margin="5,0,5,0">ObjectName</TextBlock>
            <TextBlock x:Name="TeachertName" FontSize="10" Margin="5,0,5,0">PrepodName</TextBlock>
        </StackPanel>
    </Grid>
</UserControl>
